    "nextFetchEpoch": {"type": "integer"},
    "backoffFactor": {"type": "integer", "minimum": 1},
    "lastFetchEpoch": {"type": "integer"},
    "lastError": {"type": "string"},
    "lastKnownResetEpoch": {"type": "integer"},
    "lastNotifiedResetEpoch": {"type": "integer"}
